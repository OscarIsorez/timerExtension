{
    "manifest_version": 3,
    "name": "Time Redirector",
    "version": "1.0",
    "permissions": ["storage", "tabs", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    
    "options_page": "options.html"
  }